{
    "name": "MCT2 Theme",
    "tokenColors": [
        {
            "scope": [
                "punctuation.definition.quote.mct2",
                "punctuation.definition.bracket.mct2"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "scope": "keyword.control.head.org.mct2",
            "settings": {
                "foreground": "#C586C0",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.head.trs.mct2",
            "settings": {
                "foreground": "#C586C0",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.head.desc.mct2",
            "settings": {
                "foreground": "#C586C0",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "string.other.mct2",
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "scope": "punctuation.delimiter.mct2",
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "scope": "punctuation.definition.comment.normal.head.mct2",
            "settings": {
                "foreground": "#505050"
            }
        },
        {
            "scope": "comment.line.normal.content.mct2",
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "scope": "punctuation.definition.comment.special.head.mct2",
            "settings": {
                "foreground": "#8B8B00",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "comment.line.special.content.mct2",
            "settings": {
                "foreground": "#8B8B00",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "comment.line.special.up.content.mct2",
                "comment.line.special.down.content.mct2"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "scope": [
                "punctuation.definition.insertion.begin.mct2",
                "punctuation.definition.insertion.end.mct2"
            ],
            "settings": {
                "foreground": "#D7BA7D",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "entity.name.tag.insertion.type.mct2",
            "settings": {
                "foreground": "#4EC9B0",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "string.other.insertion.content.mct2",
            "settings": {
                "foreground": "#CE9178",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword.control.head.id.mct2",
            "settings": {
                "foreground": "#4EC9B0",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.head.meta.mct2",
            "settings": {
                "foreground": "#9CDCFE",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "punctuation.separator.mct2",
            "settings": {
                "foreground": "#6A9955",
                "fontStyle": "bold"
            }
        }
    ]
}